public class persona {
	
	/**
	 * @param peso
	 * @param altura
	 */
	
	public persona () {
		
	}

	public persona(int peso, int altura) {
		// TODO Auto-generated constructor stub
	}

	public float calcularIMC(int peso, int altura) {
		
		float IMC = 0;
		
		IMC = peso / altura;
		return IMC;
		
	}
	
	public int peso;
	public int altura;
}
